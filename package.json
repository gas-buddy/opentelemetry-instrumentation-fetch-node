{
  "name": "opentelemetry-instrumentation-fetch-node",
  "version": "1.1.0",
  "description": "OpenTelemetry Node 18+ native fetch automatic instrumentation package",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "vitest",
    "build": "tsc -p tsconfig.build.json",
    "clean": "yarn dlx rimraf ./build",
    "prepare": "husky install",
    "lint": "eslint src"
  },
  "engines": {
    "node": ">18.0.0"
  },
  "author": "GasBuddy Developers <developers@gasbuddy.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/gas-buddy/opentelemetry-instrumentation-fetch-node.git"
  },
  "keywords": [
    "typescript",
    "gasbuddy",
    "opentelemetry",
    "fetch",
    "undici"
  ],
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "yarn eslint --cache --fix"
  },
  "config": {
    "coconfig": "@openapi-typescript-infra/coconfig"
  },
  "devDependencies": {
    "@openapi-typescript-infra/coconfig": "^4.2.2",
    "@opentelemetry/sdk-trace-base": "^1.17.0",
    "@opentelemetry/sdk-trace-node": "^1.17.0",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "coconfig": "^1.0.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.29.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/instrumentation": "^0.43.0",
    "@opentelemetry/semantic-conventions": "^1.17.0"
  },
  "packageManager": "yarn@3.2.4"
}
